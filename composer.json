{
    "name": "seo/analyser",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Viktor Ruskai",
            "email": "viktor@ruskai.com",
            "homepage": "http://viktor.ruskai.com/"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "6.5.8",
        "monolog/monolog": "^1.17",
        "php-di/php-di": "^6.0",
        "sentry/sdk": "^3.0",
        "slim/psr7": "^1.6.1",
        "slim/slim": "^4.0",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "App/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S 127.0.0.1:8080 -t Public",
        "tests": "phpunit --configuration phpunit.xml",
        "tests:coverage": "phpunit --configuration phpunit.xml --coverage-clover build/logs/clover.xml --coverage-html build/coverage"
    }
}
